<h1>Create a Song</h1>

<%= form_for(@song) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name%> 
  <%= f.text_field :artist_name%>

  <br />

  <%= f.collection_select(:genre_id, Genre.all, :id, :name)%>
  #collection of genres drop down menu choose with genre you want that already exist from Genre.all
  <%= f.submit %>
<% end %>


