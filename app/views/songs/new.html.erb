<h1>Create a Song</h1>

<%= form_for(@song) do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
  <%= f.label :artist_name%> <br>
  <%= f.text_field :artist_name%>
  <br />
  <%= f.collection_select(:genre_id, Genre.all, :id, :name)%><br?

  <%= f.label :note%>
  <%= f.text_field :song_notes_1%>
  <%= f.label :note%>
  <%= f.text_field :song_notes_2%>

  <%= f.submit %>
<% end %>


