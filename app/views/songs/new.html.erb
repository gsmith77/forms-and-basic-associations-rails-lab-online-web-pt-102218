<h1>Create a Song</h1>

<%= form_for(@song) do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
  <%= f.label :artist_name%> <br>
  <%= f.text_field :artist_name%>
  <br />
  <%= f.collection_select(:genre_id, Genre.all, :id, :name)%><br>
  <label> note_1 </label>
  <input type="text" id="song_notes_1" name="song[note_contents][]" </input>
  <label> note_2 </label>
  <input type="text" id="song_notes_2" name="song[note_contents][]" </input>


  <%= f.submit %>
<% end %>


